    "@zag-js/react": ">=0.3.3",
    "@zag-js/toast": ">=0.2.7",
    "ethers": ">=5.7.1",
    "react": ">=17",
    "react-dom": ">=17",
    "wagmi": ">=0.9.x"
  },
  "devDependencies": {
    "tsup": "^6.5.0",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@zag-js/react": "^0.3.3",
    "@zag-js/toast": "^0.2.7",
    "ethers": "5.7.2",
    "next": "13.1.1",
    "postcss": "^8.4.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "wagmi": "^0.10.1"
  }
