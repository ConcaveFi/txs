{
  "name": "@concave/txs-react",
  "version": "0.0.1",
  "type": "module",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./toasts/ClassicToast": "./dist/toasts/ClassicToast/ClassicToast.js",
    "./toasts/ClassicToast/styles.css": "./dist/toasts/ClassicToast/ClassicToast.css",
    "./toasts/EmojiToast": "./dist/toasts/EmojiToast/EmojiToast.js",
    "./toasts/EmojiToast/styles.css": "./dist/toasts/EmojiToast/EmojiToast.css"
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "tsup": {
    "entry": {
      "index": "src/index.ts",
      "toasts/ClassicToast": "src/toasts/ClassicToast/ClassicToast.tsx",
      "toasts/EmojiToast": "src/toasts/EmojiToast/EmojiToast.tsx"
    },
    "format": [
      "esm"
    ],
    "minify": false,
    "dts": true,
